<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <default sync-j="4" sync-c="true" revision="master"/>

  <remote fetch="git://git.yoctoproject.org" name="yocto"/>
  <remote fetch="git://git.openembedded.org" name="embedded"/>
  <remote fetch="git://github.com/freescale" name="freescale"/>
  <remote fetch="git://github.com/somlabs" name="somlabs" />
  <remote fetch="git://github.com/sgunin" name="somlabs-extra" />

  <project remote="yocto" revision="rocko" name="poky" path="sources/poky"/>
  <project remote="yocto" revision="rocko" name="meta-freescale" path="sources/meta-freescale"/>
  <project remote="embedded" revision="rocko" name="meta-openembedded" path="sources/meta-openembedded"/>
  <project remote="freescale" revision="rocko" name="meta-freescale-3rdparty" path="sources/meta-freescale-3rdparty"/>
  <project remote="freescale" revision="rocko" name="meta-freescale-distro" path="sources/meta-freescale-distro"/>
  <project remote="freescale" revision="rocko" name="Documentation" path="sources/Documentation"/>
  <project remote="freescale" revision="rocko" name="fsl-community-bsp-base" path="sources/base">
    <linkfile dest="setup-environment" src="setup-environment"/>
  </project>

  <project remote="somlabs" revision="rocko" name="meta-somlabs" path="sources/meta-somlabs"/>
  <project remote="somlabs-extra" revision="rocko" name="somlabs-extra" path="sources/meta-somlabs-extra">
    <linkfile dest="fsl-setup-release.sh" src="fsl-setup-release.sh"/>
  </project>
</manifest>
